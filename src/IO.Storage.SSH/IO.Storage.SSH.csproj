<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFrameworks>netstandard2.0;net6.0;net8.0</TargetFrameworks>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <LangVersion>latest</LangVersion>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Authors>B.Verboven</Authors>
    <RootNamespace>Regira.IO.Storage.SSH</RootNamespace>
    <PackageId>Regira.IO.Storage.SSH</PackageId>
    <AssemblyName>Regira.IO.Storage.SSH</AssemblyName>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <NoWarn>1701;1702;1591</NoWarn>
    <Version>4.0.8</Version>
  </PropertyGroup>
  <ItemGroup>
    <ProjectReference Include="..\Common.IO.Storage\Common.IO.Storage.csproj" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="SSH.NET" Version="2024.0.0" />
  </ItemGroup>
</Project>