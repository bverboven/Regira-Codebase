<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFrameworks>netstandard2.0;net8.0;net9.0</TargetFrameworks>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <LangVersion>latest</LangVersion>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Authors>B.Verboven</Authors>
    <RootNamespace>Regira.Office.PDF.PDFtoPrinter</RootNamespace>
    <PackageId>Regira.Office.PDF.PDFtoPrinter</PackageId>
    <AssemblyName>Regira.Office.PDF.PDFtoPrinter</AssemblyName>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <NoWarn>1701;1702;1591</NoWarn>
    <Version>4.7.3</Version>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="PDFtoPrinter" Version="1.5.6" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\Common.Office\Common.Office.csproj" />
  </ItemGroup>
</Project>