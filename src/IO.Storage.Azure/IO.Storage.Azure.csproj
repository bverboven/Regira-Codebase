  </PropertyGroup>
  <ItemGroup>
    <ProjectReference Include="..\Common.IO.Storage\Common.IO.Storage.csproj" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Azure.Storage.Blobs" Version="12.18.0" />
  </ItemGroup>
</Project>